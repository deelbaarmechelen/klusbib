{
	"name" : "klusbib/api",
	"description" : "A REST API to access work tool resources for use in a work tools library",
	"keywords" : [
		"klusbib",
		"rest",
		"api"
	],
	"homepage" : "http://github.com/renardeau/klusbib/api",
	"license" : "AGPLv3",
	"authors" : [{
			"name" : "Bernard Butaye",
			"email" : "klusbibmechelen@gmail.com"
		}
	],
	"require" : {
		"php" : ">=5.5.0",
		"slim/slim" : "^3.1",
		"slim/php-view" : "^2.0",
		"monolog/monolog" : "^1.17",
		"illuminate/database": "5.2.*",
		"robmorgan/phinx": "0.5.*",
		"vlucas/phpdotenv": "^2.4"
	},
	"require-dev" : {
		"phpunit/phpunit" : ">=4.8 < 6.0",
		"phing/phing": "2.*"
	},
	"autoload" : {
		"psr-4" : {
			"Api\\" : "src/classes/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	}
}
